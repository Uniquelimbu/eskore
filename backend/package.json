{
  "name": "backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "predev": "node scripts/debug/preStartCheck.js",
    "prestart": "node scripts/debug/preStartCheck.js",
    "fix-dependencies": "node scripts/fixDependencies.js",
    "lint": "eslint src/**/*.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:reset": "node ./scripts/resetDb.js",
    "db:reset:safe": "node scripts/db/resetDbSafe.js",
    "seed:generate": "npx sequelize-cli seed:generate --name",
    "migration:generate": "npx sequelize-cli migration:generate --name",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare-release": "npm run lint && npm test",
    "postinstall": "npm audit fix",
    "create-test-user": "node scripts/utils/userManagement.js create-test",
    "fix-auth-data": "node scripts/utils/userManagement.js fix-auth",
    "link-admin-athletes": "node scripts/utils/userManagement.js link-admin",
    "create:migration": "node scripts/db/createMigration.js",
    "create:sample-data": "node scripts/db/createSampleData.js",
    "test:auth": "node scripts/test/authTest.js",
    "test:serialization": "node scripts/test/serializationTest.js",
    "run:tests": "node scripts/test/runTests.js all",
    "prestart:check": "node scripts/debug/preStartCheck.js"
  },
  "keywords": [
    "sports",
    "football",
    "grassroots",
    "tracking",
    "tournaments"
  ],
  "author": "",
  "license": "ISC",
  "description": "Backend for eSkore application",
  "dependencies": {
    "axios": "^1.6.2",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "5.1.0",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^6.2.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "path-to-regexp": "6.2.1",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "router": "1.3.8",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.9",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "resolutions": {
    "path-to-regexp": "6.2.1"
  }
}
